{
	"name": "minimal-next-relay",
	"version": "1.0.0",
	"description": "Example of Next.js with Relay Modern SSR and a Graphql Server with Express",
	"scripts": {
		"schema": "graphql get-schema",
		"ngrok": "node ngrok.js",
		"d": "NODE_ENV=development node dist",
		"dev": "nodemon --exec babel-node server",
		"prebuild": "babel server --out-dir dist/server --copy-files",
		"build-server": "babel server -d dist",
		"build": "next build",
		"prestart": "npm run build",
		"start": "NODE_ENV=production node dist/server",
		"relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__generated__/**' '**/server/**' --schema ./lib/schema.graphql --verbose"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@zeit/next-css": "^1.0.1",
		"antd": "^3.23.4",
		"babel-plugin-import": "^1.12.2",
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.4",
		"dotenv": "^8.1.0",
		"express-graphql": "^0.7.1",
		"express-session": "^1.16.2",
		"formik": "^1.5.8",
		"fuse.js": "^3.4.5",
		"graphql": "^14.1.1",
		"graphql-relay": "^0.6.0",
		"isomorphic-unfetch": "^3.0.0",
		"js-cookie": "^2.2.1",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"next": "latest",
		"next-cookies": "^1.1.3",
		"null-loader": "^3.0.0",
		"passport": "^0.4.0",
		"passport-facebook": "^3.0.0",
		"passport-facebook-token": "^3.3.0",
		"passport-google-token": "^0.1.2",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-frame-component": "^4.1.1",
		"react-relay": "^5.0.0",
		"styled-components": "^4.3.2",
		"styled-normalize": "^8.0.6",
		"styled-system": "^5.1.2",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.0",
		"@babel/node": "^7.6.1",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-plugin-relay": "^2.0.0",
		"babel-plugin-styled-components": "^1.10.6",
		"concurrently": "^4.1.2",
		"graphql-cli": "^1.0.0-beta.4",
		"ngrok": "^3.2.5",
		"nodemon": "^1.19.2",
		"relay-compiler": "^6.0.0"
	}
}
